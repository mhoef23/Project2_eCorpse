<main role="main"> 
        <div class="container">
        <!-- <div id="ec_links" class="row">This is the Row for the Exquisite Corpse toolbar</div> -->
            <div class="container-fluid">
                    <div class="row">
                        <div class="col-4">
                            <div class="card">
                                <div class="card-header h3 font-weight-bold">Stories</div>
                                <div class="card-body ">
                                    <ul class="nodots">
                                      {{#each stories}}
                                        {{>stories}}
                                      {{/each}}
                                        {{!-- <li><a href="#">Story One</a></li>
                                        <li><a href="#">It Was a Dark and Stormy Night</a></li>
                                        <li><a href="#">Social Engagement</a></li>
                                        <li><a href="#">Year End Date</a></li> --}}
                                    </ul>
                                </div>
                                <div class="card-footer text-center">
                                    <ul class="nodots">
                                    <li><button class="btn btn-outline-success my-2 my-sm-0" type="submit">New Story</button></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col">
                            <div class="card">
                                <div class="card-header h3 text-center font-weight-bold">
                                    {{#each stories}}
                                    {{#if id}}SLEEP{{/if}}
                                        {{/each}}
                                <div class="card-body h4" id="last-paragraph">
                                    {{#each elements}}
                                      {{>elements}}
                                    {{/each}}
                                </div>
                                <div class="card-footer text-center">
                                        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Add to the Corpse</button>
                                </div>
                            </div>

                        </div>
                    </div>
        </div>
    </main>